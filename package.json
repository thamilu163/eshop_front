{
  "name": "ecommerce-enterprise-frontend",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.8.0",
  "engines": {
    "node": ">=24.12.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "build": "npm run build:sw && next build",
    "build:sw": "tsc --project tsconfig.sw.json",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "clean": "rimraf .next .turbopack node_modules/.cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test",
    "check:env": "tsx ./scripts/validate-env.ts",
    "check:icons": "tsx ./scripts/generate-lucide-types.ts --check",
    "check:keycloak": "tsx ./scripts/verify-keycloak-setup.ts",
    "gen:lucide-types": "tsx ./scripts/generate-lucide-types.ts",
    "ci:preflight": "npm run check:env",
    "ci": "npm run ci:preflight && npm run validate && npm run build",
    "prepare": "husky"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.32.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@upstash/redis": "^1.36.0",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "idb-keyval": "^6.2.2",
    "isomorphic-dompurify": "^2.34.0",
    "jose": "^6.1.3",
    "libphonenumber-js": "^1.12.33",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "sonner": "^2.0.7",
    "stripe": "^20.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.0",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^7.2.0",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
